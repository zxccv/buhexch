			СтруктураВыгрузки.Вставить("ВидРасходовНУ", XMLСтрока(СсылкаНаОбъект.ВидРасходов));		
		КонецЕсли;
	//Самойлов А.С. Конец  2022-09-27	
	Иначе	
	КонецЕсли;	
	
	НовОб = ТаблицаВыгрузки.Добавить();
	НовОб.СсылкаНаОбъект = СсылкаНаОбъект;
