	Запрос.УстановитьПараметр("Дата", Новый Граница(Дата(2020, 12, 31, 23, 59, 59), ВидГраницы.Включая));
	
	Организации = Новый Массив;
	Организации.Добавить(Справочники.Организации.НайтиПоНаименованию("Тепло3000", Истина));
